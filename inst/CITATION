citHeader("To cite package 'poolfstat' in publications use:")

year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date)
vers <- paste("R package version", meta$Version)


citEntry(entry="Article",
         author = 	 "Hivert V., Leblois R., Petit E.J., Gautier M., and Vitalis R.",
         title = 	 "Measuring genetic differentiation from Pool-seq data",
         journal = 	 "Genetics",
         year = 	 0,
         volume =	 0,
         number =	 "submitted",
         pages="1176-1177",
         textVersion ="Hivert V, Leblois R, Petit EJ, Gautier M, and Vitalis R (2018). Measuring genetic differentiation from Pool-seq data. Genetics, submitted" )
